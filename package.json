{
  "name": "iteacher",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "https://github.com/ProgramadorRanzinza/ITeacher.git",
  "author": "Luiz Pires <kw13code@gmail.com>",
  "license": "MIT",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "lint": "eslint **/*.ts",
    "format": "prettier --write **/*.{json,ts}",
    "test": "jest --passWithNoTests",
    "test:coverage": "jest --coverage --passWithNoTests",
    "test:unit": "jest --passWithNoTests -c ./jest-unit-config.js",
    "test:unit:watch": "jest --passWithNoTests -c ./jest-unit-config.js",
    "test:integration": "jest -c --passWithNoTests ./jest-integration-config.js",
    "preBuild": "rm -rf build",
    "build": "tsc",
    "start:api": "yarn workspace @iteacher/api start:api",
    "typeorm": "yarn workspace @iteacher/api typeorm"
  },
  "dependencies": {
    "@iteacher/eslint-config": "*",
    "jest": "26.4.2",
    "ts-jest": "^26.1.3",
    "typescript": "^4.0.2"
  },
  "devDependencies": {
    "@types/supertest": "^2.0.10",
    "@types/jest": "26.0.12",
    "supertest": "^4.0.2"
  }
}
