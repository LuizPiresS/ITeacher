{
  "name": "iteacher-api",
  "version": "0.0.1",
  "main": "index.js",
  "repository": "https://github.com/ProgramadorRanzinza/ITeacher.git",
  "author": "Luiz Pires <kw13code@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "yarn build && node dist/index.js",
    "start:dev": "tsnd --respawn --exit-child --debounce --clear --transpile-only --inspect --no-notify src/main.ts",
    "preBuild": "rm -rf build",
    "build": "tsc",
    "lint": "eslint **/*.ts",
    "lint:fix": "eslint ./src --ext .ts --fix",
    "test:unit": "jest --passWithNoTests -c ./jest-unit-config.js",
    "test:end2end": "jest  --passWithNoTests --runInBand -c './jest-integration-config.js'",
    "test:coverage": "yarn test:end2end --coverage && yarn test:unit --coverage",
    "db:migration:create": "typeorm migration:create",
    "db:migration:run": "typeorm migration:run",
    "db:migration:revert": "typeorm migration:revert"
  },
  "dependencies": {
    "@brazilian-utils/brazilian-utils": "^1.0.0-rc.7",
    "config": "^3.3.1",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "fast-xml-parser": "^3.17.2",
    "jsonwebtoken": "^8.5.1",
    "pg": "^8.3.0",
    "pino-pretty": "^4.0.0",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.2.25"
  },
  "devDependencies": {
    "@types/config": "^0.0.36",
    "@types/express": "^4.17.7",
    "@types/jest": "26.0.12",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/node": "^14.0.5",
    "@types/pg": "^7.14.4",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^4.3.0",
    "@typescript-eslint/parser": "^4.3.0",
    "eslint": "^7.10.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-import-helpers": "^1.1.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "git-commit-msg-linter": "^2.4.2",
    "husky": "^4.2.5",
    "jest": "26.4.2",
    "lint-staged": "^10.2.6",
    "supertest": "^4.0.2",
    "ts-jest": "^26.1.3",
    "ts-node-dev": "^1.0.0-pre.56",
    "typescript": "^4.0.2"
  }
}
